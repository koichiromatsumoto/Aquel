
<div class="container-fluid user_posts">
	<div class="row">
		<div class="col-md-12">
			<% if @user.posts.exists? %>
				<% @user.posts.each do |post| %>
					<%= attachment_image_tag post, :post_image, format:"jpg", fallback:"no_image.jpg", class:"post-show-img" %>
				<% end %>
			<% else %>
			    <h4>投稿はありません</h4>
			<% end %>
		</div>
	</div>
</div>